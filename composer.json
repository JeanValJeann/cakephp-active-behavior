{
    "name": "jeanvaljean/active-behavior",
    "description": "Active plugin for CakePHP",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "jeanvaljean",
        "active",
        "behavior"
    ],
    "homepage": "https://github.com/JeanValJeann/cakephp-active-behavior",
    "license": "MIT",
    "authors": [
        {
            "name": "PERRIN Jean-Charles",
            "homepage": "https://github.com/JeanValJeann",
            "role": "Author"
        }
    ],
    "support": {
        "issues": "https://github.com/JeanValJeann/cakephp-active-behavior/issues",
        "source": "https://github.com/JeanValJeann/cakephp-active-behavior"
    },
    "require": {
        "php": ">=5.6.0",
        "cakephp/orm": "~3.0"
    },
    "require-dev": {
        "cakephp/cakephp": "^3.6",
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Active\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Active\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
